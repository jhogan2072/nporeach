<% if target %>
  <% if target.errors.any? %>
    <div id="error_explanation">
      <h2><%= raw t('errors.template.header', count: target.errors.count, model: model) %></h2>

      <ul>
      <% target.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
<% end %>
